{
  "name": "terraform-graph",
  "displayName": "Terraform Graph",
  "description": "Visual representation of a Terraform plan, showing added, deleted and updated resources",
  "version": "1.4.1",
  "license": "MIT",
  "author": {
    "name": "Sara Morillon",
    "url": "https://saramorillon.com"
  },
  "repository": {
    "url": "https://github.com/saramorillon/terraform-graph"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "publisher": "saramorillon",
  "contributes": {
    "customEditors": [
      {
        "viewType": "terraform-graph.view",
        "displayName": "Terraform graph",
        "selector": [
          {
            "filenamePattern": "*.tfgraph"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "terraform-graph.generate",
        "title": "Generate Terraform graph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint \"{src,public}/**/*.{ts,tsx,js}\"",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^8.42.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
