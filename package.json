{
  "name": "terraform-graph",
  "displayName": "terraform-graph",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/index.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "terraform-graph.view",
        "displayName": "Terraform graph",
        "selector": [
          {
            "filenamePattern": "*.tfgraph"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "terraform-graph.show",
        "title": "Show Terraform graph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts,js",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "typescript": "^5.1.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  }
}
